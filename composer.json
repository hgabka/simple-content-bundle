{
    "name": "hgabka/simple-content-bundle",
    "type": "symfony-bundle",
    "description": "Simple content bundle",
    "keywords": [
    ],
    "homepage": "https://github.com/hgabka/simple-content-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabe",
            "email": "hgabka@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=8.0.2",
        "symfony/framework-bundle": "^6.0",
        "hgabka/utils-bundle": "^6.0",
        "hgabka/doctrine-translatable-bundle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Hgabka\\SimpleContentBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hgabka\\SimpleContentBundle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
